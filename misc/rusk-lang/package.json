{
  "name": "rusk-lang",
  "displayName": "Rusk Language Support",
  "description": "Syntax highlighting for the Rusk programming language",
  "version": "0.0.1",
  "repository": {
    "url": "https://github.com/windoze/rusk"
  },
  "engines": {
    "vscode": "^1.109.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "rusk",
      "aliases": ["Rusk", "rusk"],
      "extensions": [".rusk"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "rusk",
      "scopeName": "source.rusk",
      "path": "./syntaxes/rusk.tmLanguage"
    }]
  }
}
