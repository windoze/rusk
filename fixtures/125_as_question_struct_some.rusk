// expect: ok int 1

interface Animal { fn kind() -> int; }

struct Dog {}
struct Cat {}

impl Animal for Dog { fn kind(self: Dog) -> int { 1 } }
impl Animal for Cat { fn kind(self: Cat) -> int { 2 } }

fn main() -> int {
    let a = (Dog {} as Animal);
    match a as? Dog {
        Option::Some(_dog) => 1,
        Option::None => 0,
    }
}

