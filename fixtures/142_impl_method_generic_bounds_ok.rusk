// expect: ok int 7

trait Show { fn show() -> string; }

interface Pair<T> {
    fn pair<U: Show>(u: U) -> (T, U);
}

struct Box<T> { v: T }
struct S { x: int }

impl Show for S { fn show() -> string { "S" } }

impl<T> Pair<T> for Box<T> {
    fn pair<U: Show>(u: U) -> (T, U) { (self.v, u) }
}

fn main() -> int {
    let b: Box<int> = Box { v: 7 };
    let p = (b as Pair<int>).pair(S { x: 0 });
    p.0
}
